#include <iostream>
using namespace std;
int main()
{
    char c;
    cout<<"Ingrese un caracter C: "; cin>>c;
    if(c>=65 && c<=90){
        c+=32;
        cout<<"Letra convertida: "<<c;
    }
    else if(c>=97 && c<=122){
        c-=32;
        cout<<"Letra convertida: "<<c;
    }
    else{
        cout<<"Lo ingresado no es una letra.";
    }

    return 0;
}
